{
  "expo": {
    "name": "backlog",
    "slug": "backlog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "backlog",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fedecrovetto.MinimalistBacklog"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eba1293f-b2e2-484e-82ae-342e03c8d784"
      },      
      "ApiUrl": "https://func-backlogapp-prod.azurewebsites.net/api/",
      "GetTasksByUser": "getTasksByUser",
      "CreateTask": "getAllTasks",
      "UpdateTask": "UpdateTask",
      "PostUser": "PostUser",
      "PostTask": "PostTask",
      "FileName": "backlogItems.json",
      "GoogleClientID": "994492199508-65j4cn8hb7h36aslesl4p6583hpbg97j.apps.googleusercontent.com"      
    }
  }
}
